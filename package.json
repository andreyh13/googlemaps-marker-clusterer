{
  "name": "googlemaps-marker-clusterer",
  "version": "1.0.0",
  "description": "Clusterer library for Google Maps JavaScript API v3 markers",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "webpack",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreyh13/googlemaps-marker-clusterer.git"
  },
  "keywords": [
    "googlemaps",
    "clusterer",
    "markers"
  ],
  "author": "Andriy Khomenko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andreyh13/googlemaps-marker-clusterer/issues"
  },
  "homepage": "https://github.com/andreyh13/googlemaps-marker-clusterer#readme",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "jest": "^25.4.0",
    "prettier": "^2.0.5",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@types/googlemaps": "^3.39.3"
  },
  "files": [
    "lib/**/*"
  ]
}
